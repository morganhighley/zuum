{
  "name": "Zuum Media H10X10-4K6G",
  "version": "1.0.0",
  "manufacturer": "Zuum Media",
  "model": "H10X10-4K6G",
  "type": "MatrixSwitcher",
  "description": "4K60Hz 10x10 HDMI Matrix Switcher with Audio Extraction",
  "author": "Crestron Driver",
  "connectionType": "serial",
  "communication": {
    "protocol": "RS232",
    "baudRate": 9600,
    "dataBits": 8,
    "stopBits": 1,
    "parity": "None",
    "delimiter": "\r\n"
  },
  "commands": {
    "powerOn": {
      "command": "POWER 01\r\n",
      "description": "Power On Matrix"
    },
    "powerOff": {
      "command": "POWER 00\r\n",
      "description": "Power Off Matrix (Standby)"
    },
    "routeInput": {
      "command": "TX{output} {input}\r\n",
      "description": "Route input to output",
      "parameters": {
        "output": {
          "type": "integer",
          "min": 1,
          "max": 10
        },
        "input": {
          "type": "integer",
          "min": 0,
          "max": 10,
          "note": "0 = OFF"
        }
      }
    },
    "audioSelect": {
      "command": "AUDIO{output} {source}\r\n",
      "description": "Select audio source for SPDIF output",
      "parameters": {
        "output": {
          "type": "integer",
          "min": 1,
          "max": 2,
          "note": "SPDIF output 1 or 2"
        },
        "source": {
          "type": "string",
          "values": ["H", "A"],
          "note": "H=HDMI, A=ARC"
        }
      }
    },
    "saveScene": {
      "command": "SAVE {scene}\r\n",
      "description": "Save current configuration to scene",
      "parameters": {
        "scene": {
          "type": "integer",
          "min": 1,
          "max": 10
        }
      }
    },
    "loadScene": {
      "command": "LOAD {scene}\r\n",
      "description": "Load saved scene configuration",
      "parameters": {
        "scene": {
          "type": "integer",
          "min": 1,
          "max": 10
        }
      }
    },
    "lockPanel": {
      "command": "LOCK {state}\r\n",
      "description": "Lock/Unlock front panel and IR remote",
      "parameters": {
        "state": {
          "type": "integer",
          "values": [0, 1],
          "note": "0=Unlock, 1=Lock"
        }
      }
    },
    "setIRID": {
      "command": "IRID {id}\r\n",
      "description": "Set IR Remote ID",
      "parameters": {
        "id": {
          "type": "integer",
          "min": 0,
          "max": 9
        }
      }
    },
    "setDHCP": {
      "command": "DHCP {state}\r\n",
      "description": "Enable/Disable DHCP Client",
      "parameters": {
        "state": {
          "type": "integer",
          "values": [0, 1],
          "note": "0=Disable, 1=Enable"
        }
      }
    },
    "setIPAddress": {
      "command": "IP_ADDRESS {ip}\r\n",
      "description": "Set IP Address",
      "parameters": {
        "ip": {
          "type": "string",
          "format": "xxx.xxx.xxx.xxx"
        }
      }
    },
    "setSubnetMask": {
      "command": "SUBNET_MASK {mask}\r\n",
      "description": "Set Subnet Mask",
      "parameters": {
        "mask": {
          "type": "string",
          "format": "xxx.xxx.xxx.xxx"
        }
      }
    },
    "setGateway": {
      "command": "GATEWAY {gateway}\r\n",
      "description": "Set Gateway",
      "parameters": {
        "gateway": {
          "type": "string",
          "format": "xxx.xxx.xxx.xxx"
        }
      }
    },
    "setMediaType": {
      "command": "MEDIA_TYPE {type}\r\n",
      "description": "Set Network Media Type",
      "parameters": {
        "type": {
          "type": "integer",
          "values": [0, 1, 2],
          "note": "0=Auto, 1=10M, 2=100M"
        }
      }
    },
    "setMACFilter": {
      "command": "MAC_FILTER {state}\r\n",
      "description": "Enable/Disable MAC Filtering",
      "parameters": {
        "state": {
          "type": "integer",
          "values": [0, 1],
          "note": "0=Disable, 1=Enable"
        }
      }
    },
    "getStatus": {
      "command": "STATUS\r\n",
      "description": "Get connection status"
    },
    "getVersion": {
      "command": "VERSION\r\n",
      "description": "Get firmware version"
    },
    "getHelp": {
      "command": "HELP\r\n",
      "description": "Get command help"
    }
  },
  "properties": {
    "power": {
      "type": "boolean",
      "description": "Power state",
      "readOnly": false
    },
    "routing": {
      "type": "object",
      "description": "Current routing configuration",
      "properties": {
        "output1": {"type": "integer", "min": 0, "max": 10},
        "output2": {"type": "integer", "min": 0, "max": 10},
        "output3": {"type": "integer", "min": 0, "max": 10},
        "output4": {"type": "integer", "min": 0, "max": 10},
        "output5": {"type": "integer", "min": 0, "max": 10},
        "output6": {"type": "integer", "min": 0, "max": 10},
        "output7": {"type": "integer", "min": 0, "max": 10},
        "output8": {"type": "integer", "min": 0, "max": 10},
        "output9": {"type": "integer", "min": 0, "max": 10},
        "output10": {"type": "integer", "min": 0, "max": 10}
      }
    },
    "audioSources": {
      "type": "object",
      "description": "SPDIF audio source selection",
      "properties": {
        "spdif1": {"type": "string", "enum": ["H", "A"]},
        "spdif2": {"type": "string", "enum": ["H", "A"]}
      }
    },
    "panelLocked": {
      "type": "boolean",
      "description": "Front panel lock state"
    },
    "irID": {
      "type": "integer",
      "description": "IR Remote ID",
      "min": 0,
      "max": 9
    },
    "version": {
      "type": "string",
      "description": "Firmware version",
      "readOnly": true
    }
  },
